# Copy to .env.local for local development
# DATABASE_URL is read from environment - if not set, defaults to SQLite

# Local PostgreSQL (Docker) - optional
# DATABASE_URL="postgresql://triage:triage@localhost:5432/triage_db"

# OpenAI API key (required for voice pipeline)
OPENAI_API_KEY=

# OpenAI model configuration
OPENAI_MODEL_TEXT=gpt-4o-mini
OPENAI_MODEL_STT=gpt-4o-mini-transcribe
OPENAI_MODEL_TTS=gpt-4o-mini-tts
OPENAI_MODEL_REALTIME=gpt-4o-mini-realtime-preview

# Active domains (comma-separated)
ACTIVE_DOMAINS=medical

# CORS origins (comma-separated, for production)
# CORS_ORIGIN=https://your-domain.vercel.app
